{
	"name": "svendefront",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=20 <25"
	},
	"scripts": {
		"dev": "cross-env ROLLUP_WASM=true vite dev",
		"build": "cross-env ROLLUP_WASM=true svelte-kit sync && cross-env ROLLUP_WASM=true vite build",
		"preview": "vite preview",
		"check": "cross-env ROLLUP_WASM=true svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "cross-env ROLLUP_WASM=true svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"cross-env": "^7.0.3",
		"@sveltejs/adapter-auto": "^6.1.0",
		"@sveltejs/kit": "^2.43.2",
		"@sveltejs/vite-plugin-svelte": "^6.2.0",
		"svelte": "^5.39.5",
		"svelte-check": "^4.3.2",
		"typescript": "^5.9.2",
		"vite": "^7.1.7",
		"packageManager": "pnpm@9"
	}
}
